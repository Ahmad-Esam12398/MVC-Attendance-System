@model Attendance_Management_System.Models.ITIProgram
@section NavList
{

    @Html.Partial("navBar")


}

<h2>@ViewData["Title"]</h2>

<form asp-action="CreateProgram" method="post">
    <div class="form-group">
        <label asp-for="Id" class="control-label"></label>
        <input asp-for="Id" class="form-control" disabled />
        <span asp-validation-for="Id" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Name" class="control-label"></label>
        <input asp-for="Name" class="form-control" />
        <span asp-validation-for="Name" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Description" class="control-label"></label>
        <input asp-for="Description" class="form-control" />
        <span asp-validation-for="Description" class="text-danger"></span>
    </div>
    <div class="form-group">
        <div class="form-check">
            <input class="form-check-input" type="checkbox" asp-for="IsActive" />
            <label class="form-check-label" asp-for="IsActive">Is Active</label>
        </div>
    </div>
    <div class="form-group">
        <input type="submit" value="@ViewData["Button"]" class="btn btn-primary" />
    </div>
</form>
