@model Attendance_Management_System.Models.Track



<div>

    <dl class="row">
        <dt class="col-sm-2">
          Track ID : 
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Id)
        </dd>
        <dt class="col-sm-2">
           Name : 
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Name)
        </dd>
        <dt class="col-sm-2">
           Status : 
        </dt>
      
            @if (Model.IsActive == true)
            {
            <dd class="col-sm-10">
                Active
            </dd>
            }
            else
            {
            <dd class="col-sm-10">
                Inactive
            </dd>
            }
       
        <dt class="col-sm-2">
           Program : 
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Program.Name)
        </dd>
        <dt class="col-sm-2">
          Supervisor : 
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Supervisor.UserName)
        </dd>
    </dl>
      
    </div>