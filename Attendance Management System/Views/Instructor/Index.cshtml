 @{
     ViewData["Title"] = "Instructor";
     Layout = "_Layout";
     bool IsSuperVisor = (bool)ViewData["IsSuperVisor"];
 }

 @section NavList
 {
     @if(IsSuperVisor)
     {
         <div class="nav_list">
             <a href="/instructor/permissions" class="nav_link">
                 <i class="bx bx-message-square-detail nav_icon"></i>
